{
  "name": "@mindfulstudio/ms-web-live-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "nodemon",
    "build": "next build && tsc --project tsconfig.server.json",
    "start": "node dist/index.js",
    "typecheck": "tsc --noEmit",
    "deploy:stage": "VERCEL_PROJECT_ID=$VERCEL_PROJECT_ID_STAGE vercel deploy --no-clipboard --token=$VERCEL_TOKEN --prod",
    "deploy:prod": "VERCEL_PROJECT_ID=$VERCEL_PROJECT_ID_PROD vercel deploy --no-clipboard --token=$VERCEL_TOKEN --prod"
  },
  "dependencies": {
    "@apollo/client": "^3.3.6",
    "@mindfulstudio/ms-web-live-tsconfig": "0.1.0",
    "@mindfulstudio/ms-web-live-types": "0.1.0",
    "@mindfulstudio/ms-web-live-ui": "0.1.0",
    "@next/bundle-analyzer": "^12.0.3",
    "graphql": "^15.4.0",
    "next": "12.0.3",
    "next-compose-plugins": "^2.2.1",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "socket.io": "^4.3.2"
  },
  "devDependencies": {
    "@emotion/babel-plugin": "^11.3.0",
    "@types/node": "^14.14.13",
    "@types/react": "^17.0.34",
    "next-transpile-modules": "^9.0.0",
    "nodemon": "^2.0.14",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  }
}
